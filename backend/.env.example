# MobileAgent Backend Configuration
# Copy this file to .env and modify as needed

# Server settings
MOBILEAGENT_HOST=0.0.0.0
MOBILEAGENT_PORT=8000
MOBILEAGENT_DEBUG=false

# CORS origins (comma-separated)
MOBILEAGENT_CORS_ORIGINS=["http://localhost:5173","http://localhost:3000"]

# Observer settings
MOBILEAGENT_POLL_INTERVAL=1.0
MOBILEAGENT_DISCOVERY_INTERVAL=5.0
MOBILEAGENT_CAPTURE_LINES=500

# Optional: tmux socket path (for non-default tmux servers)
# MOBILEAGENT_TMUX_SOCKET=/tmp/tmux-1000/default
